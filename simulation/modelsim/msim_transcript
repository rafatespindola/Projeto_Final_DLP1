add wave -position insertpoint sim:/gera_baudrate/*
force -freeze sim:/gera_baudrate/clk_gbd 1 0, 0 {10000 ps} -r 20ns
run 19ns
run 1ns
run 1ns
force -freeze sim:/gera_baudrate/rst_gbd 1 0 -cancel 40ns
run 20ns
run 20ns
run 20ns


run 20ns
run 20ns
run 20ns
run 20ns
run 20ns
run 20ns
